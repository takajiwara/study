
<?php

echo "FizzBuzz問題";
echo PHP_EOL;

for($i = 1; $i <=100; $i++){
    if($i % 15 === 0){
        echo "FizzBuzz";
        echo PHP_EOL;
    }elseif($i % 5 === 0){
        echo "Buzz";
        echo PHP_EOL;
    }elseif($i % 3 === 0){
        echo "Fizz";
        echo PHP_EOL;
    }else{
        echo $i;
        echo PHP_EOL;
    }
}

?>
